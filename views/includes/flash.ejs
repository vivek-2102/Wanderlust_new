<% if(success&&success.length){ %>
<div class="alert-success alert alert-dismissible fade show col-6 offset-3" role="alert">
 <%= success %>
 <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
<% if(error&&error.length){ %>
<div class="alert-danger alert alert-dismissible fade show col-6 offset-3" role="alert">
 <%= error %>
 <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
